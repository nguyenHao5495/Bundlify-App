       <Button plain monochrome onClick={changeSelected}>\r\n                                        here to\r\n                                    </Button>\r\n                                to create one.\r\n                              </div>\r\n\r\n                            }\r\n\r\n                            <TextField value={searchTerm} onChange={onChangeSearch} placeholder=\"Search by bundle name...\" />\r\n                            <Table dataSource={data} rowKey={data => data.key}>\r\n                                <Column\r\n                                    dataIndex=\"name\"\r\n                                    key=\"name\"\r\n                                />\r\n                                <Column\r\n                                    dataIndex=\"switch\"\r\n                                    key=\"switch\"\r\n                                    render={(text, record) => (\r\n                                        <div className=\"text-center\">\r\n                                            <Switch checked={text} onChange={(value) => { onChangeSwitch(value, record.key) }} />\r\n                                        </div>\r\n\r\n                                    )} />\r\n                                <Column\r\n                                    dataIndex=\"name\"\r\n                                    key=\"name\"\r\n                                    render={(text, record) => (\r\n                                        <div className=\"text-center\">\r\n                                            <Space size=\"middle\">\r\n                                                <a className=\"btn_table\" href=\"# \" onClick={() => changeEdit(record)}>Edit <Icon source={EditMinor} /></a>\r\n                                                <div className=\"delete-rule\">\r\n                                                    <Popconfirm title=\"Sure to delete?\" onConfirm={() => handleDelete(record.key)}>\r\n                                                        <a className=\"btn_table\" href=\"javascriptvoid(0)\">Delete <Icon source={DeleteMinor} /></a>\r\n                                                    </Popconfirm>\r\n                                                </div>\r\n                                            </Space>\r\n\r\n                                        </div>\r\n\r\n                                    )} />\r\n                            </Table>\r\n                        </Layout.Section>\r\n                    </Layout>\r\n                </Card>\r\n                <div style={{ width: '100%' }}>\r\n                    <Modal\r\n                        open={active}\r\n                        onClose={changeEdit}\r\n                        className=\"abc\"\r\n                        title=\"Edit bundle\"\r\n                        primaryAction={{\r\n                            content: 'SAVE',\r\n                            onAction: changeEdit,\r\n                        }}\r\n                        secondaryActions={[\r\n                            {\r\n                                content: 'CLOSE',\r\n                                onAction: changeEdit,\r\n                            },\r\n                        ]}\r\n                        large={true}\r\n                    >\r\n                        <Modal.Section>\r\n                            <Bundle />\r\n\r\n                        </Modal.Section>\r\n                    </Modal>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default AllOrder;\r\n\r\n","C:\\xampp\\htdocs\\omegashopifyapp\\admin\\myapp\\src\\pages\\Bundle.js",[],"C:\\xampp\\htdocs\\omegashopifyapp\\admin\\myapp\\src\\reducers\\index.js",[],"C:\\xampp\\htdocs\\omegashopifyapp\\admin\\myapp\\src\\components\\createData.js",["180","181"],"C:\\xampp\\htdocs\\omegashopifyapp\\admin\\myapp\\src\\components\\ListProduct.js",[],"C:\\xampp\\htdocs\\omegashopifyapp\\admin\\myapp\\src\\c